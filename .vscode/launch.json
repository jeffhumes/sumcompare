    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug JavaFX App",
            "request": "launch",
            "mainClass": "org.bofus.sumcompare.gui.SumCompareGUI",
            "vmArgs": "--module-path /usr/share/openjfx/lib --add-modules javafx.controls,javafx.fxml",
            "projectName": "sumcompare" 
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Dry Run with MD5",
            "request": "launch",
            "mainClass": "org.bofus.sumcompare.Main",
            "projectName": "sumcompare",
            "args": ["-d", "-k", "-z", "MD5", "-s", "/home/jeffhumes/ORIGINAL_VIDEOS/PHONE_DCIM","-t", "/home/jeffhumes/ORIGINAL_VIDEOS/MOTORCYCLES"]
        },
        {
            "type": "java",
            "name": "Dry Run with XXHASH64",
            "request": "launch",
            "mainClass": "org.bofus.sumcompare.Main",
            "projectName": "sumcompare",
            "args": ["-d", "-k", "-z", "XXHASH64", "-s", "/home/jeffhumes/ORIGINAL_VIDEOS/PHONE_DCIM","-t", "/home/jeffhumes/ORIGINAL_VIDEOS/MOTORCYCLES"]
        }
    ]
}